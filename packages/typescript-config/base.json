{
	"compilerOptions": {
		"moduleResolution": "Bundler",
		"module": "ESNext",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "es2022",
		"allowJs": true,
		"resolveJsonModule": true,
		"moduleDetection": "force",
		"isolatedModules": true,
		"noEmit": true,
		"verbatimModuleSyntax": true
	}
}
